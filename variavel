        char sexo='f';
/* 
        System.out.println(nome);
        System.out.println(idade);
        System.out.println(altura);
        System.out.println(sexo);
   */ 

        System.out.println("Olá "+nome+"sua idade é"+idade+
        "Sua altura é"+altura+"seu sexo é"+sexo);

  }
}
